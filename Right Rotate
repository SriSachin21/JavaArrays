import java.util.Arrays;
public class Main
{
	public static void main(String[] args) {
	    int a[]={9,8,7,6,5,4,3};
	    System.out.println(Arrays.toString(a));
	    int b=a[a.length-1];
	    int c[]=new int [a.length];
	    int d;
	    for(int i=a.length-1;i>0;i--)
	    {
	        c[i]=a[i-1];
	    }
	    c[0]=b;
	    for(int x:c)
	    System.out.print(x+",");
	}
}
