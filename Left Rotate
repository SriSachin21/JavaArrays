import java.util.Arrays;
public class Main
{
	public static void main(String[] args) {
	    int a[]={1,2,3,4,5,6,7};
	    System.out.println(Arrays.toString(a));
	    int b=a[0];
	    int c[]=new int [a.length];
	    int d;
	    for(int i=0;i<a.length-1;i++)
	    {
	        c[i]=a[i+1];
	    }
	    c[a.length-1]=b;
	    for(int x:c)
	     System.out.print(x+" ,");
	}
}
