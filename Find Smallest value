import java.util.Arrays;
public class Main{
    public static void main(String args[])
    {
        
        int basearray[]={11,3,2,4,5,7,6,49,8,31,2,59,7};
        int check=basearray[0];
       for(int i=0;i<basearray.length;i++)
       {
           if(check>basearray[i])
           {
               check=basearray[i];
           }
           
       }System.out.println(check);
    }
}
